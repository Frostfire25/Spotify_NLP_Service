    "@emotion/styled": "^11.10.8",
    "@mui/material": "^5.12.3",
    "bootstrap": "4.3.1",
    "classnames": "2.3.1",
    "dotenv": "^16.4.5",
    "headroom.js": "0.12.0",
    "moment": "2.29.4",
    "nouislider": "14.1.1",
    "react": ">=16.0.0",
    "react-datetime": "3.1.1",
    "react-dom": ">=16.0.0",
    "react-google-charts": "^4.0.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "^5.0.1",
    "reactstrap": "8.4.1",
    "sass": "1.53.0"
  },
